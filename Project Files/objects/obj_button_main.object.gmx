image_index=0;
inst_mult_yes=false;
scr_fade_in();
//makes center button larger
image_xscale=1.5;
image_yscale=1.5;
//controls color change
if global.active=true {
    delay = irandom_range(20,50);
    if change=true {
        while global.color_id=global.rng {
            global.rng=ceil(irandom(level_check));
        }
        sprite_index=global.inv_color[global.rng];
        global.color_id=global.rng;
        global.derp_rng=irandom_range(0,100);
        //controls powerup probability
        if global.derp_rng&gt;=global.derp_chance &amp;&amp; global.derp=false {
            scr_create_powerup_freeze();
        }
        global.double_rng=irandom_range(0,100);
        if global.double_rng&gt;=global.double_chance &amp;&amp; double=false {
            scr_create_powerup_double();
        }
        change=false;
    }
    
    //adds points
    if global.color_id_check=global.color_id {
        change=true;
        global.x3rng=irandom_range(0,100);
        global.x4rng=irandom_range(0,100);
        global.x5rng=irandom_range(0,100);
        if global.x3rng&gt;=global.x3_chance &amp;&amp; inst_mult_yes=false {
            global.instant_mult=3;
            instance_create(375,667,obj_x3);
            inst_mult_yes=true;
        }
        if global.x4rng&gt;=global.x4_chance &amp;&amp; inst_mult_yes=false {
            global.instant_mult=4;
            instance_create(375,667,obj_x4);
            inst_mult_yes=true;
        }
        if global.x5rng&gt;=global.x5_chance &amp;&amp; inst_mult_yes=false {
            global.instant_mult=5;
            instance_create(375,667,obj_x5);
            inst_mult_yes=true;
        }
        global.points+=ceil(((ceil(global.multiplier)*global.instant_mult*global.point_multiplier)*more_buttons_multiplier));
        instance_create(mouse_x,mouse_y,obj_score_popup);
        global.instant_mult=1;    
    }
    //powerup timers
    if global.derp=true {
        timer-=1;
    }
    
    if double=true {
        global.point_multiplier=2;
        double_timer-=1;
    }   
    
    if timer&lt;=0 {
        global.derp=false;
        timer=global.timer_edit;
    }
    
    if double_timer&lt;=0 {
        global.point_multiplier=1;
        double=false;
        double_timer=global.timer_edit;
    }
    
    //pig pop-up
    global.pig_rng=irandom_range(0,100);
    if global.pig_rng&gt;=global.pig_chance {
        scr_create_powerup_pig();
    }
}
if fade=true {
    delay=0.05;
}
if global.fade_out=true {
    image_alpha-=(delay)
    if image_alpha&lt;=0 &amp;&amp; fade=true {
        room_goto(rm_game);
        global.fade_out=false;
    }
}
else
{
scr_fade_in();
}
